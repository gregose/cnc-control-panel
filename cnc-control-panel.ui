<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="Main_Frame">
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">1</property>
      <rows>
        <row id="0">
          <property name="pad">0</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="ttk.Button" id="Button_Stepper">
        <property name="command">on_stepper_clicked</property>
        <property name="compound">left</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Stepper Motors</property>
        <property name="width">30</property>
        <layout>
          <property name="column">0</property>
          <property name="ipady">20</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Button_Water">
        <property name="command">on_water_clicked</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Water Pump</property>
        <property name="width">30</property>
        <layout>
          <property name="column">0</property>
          <property name="ipady">20</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Button_Vacuum">
        <property name="command">on_vacuum_clicked</property>
        <property name="compound">bottom</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Vacuum</property>
        <property name="width">30</property>
        <layout>
          <property name="column">0</property>
          <property name="ipady">20</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Button_Outlet">
        <property name="command">on_outlet_clicked</property>
        <property name="compound">bottom</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Outlet</property>
        <property name="width">30</property>
        <layout>
          <property name="column">0</property>
          <property name="ipady">20</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="Text_Status">
        <property name="height">8</property>
        <property name="insertwidth">0</property>
        <property name="state">disabled</property>
        <property name="width">34</property>
        <property name="wrap">word</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
